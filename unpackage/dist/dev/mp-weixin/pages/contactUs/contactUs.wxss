page{
  background:#eee;
  position:relative;
  height:100%;
}
.cell{
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  width:100%;
  min-height:144rpx;
  background: #fff;
  border-bottom:1rpx solid #eee;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding:24rpx;
}
.shape{
  width:8rpx;
  height:40rpx;
  display:inline-block;
  background:rgba(246,174,106,1);
  margin-right:30rpx;
}
.content view:nth-of-type(1){
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.content view:nth-of-type(2){
  font-size:28rpx;
  margin-top:30rpx;
}
.code{
  text-align:center;
  width:100%;
  background:#fff;
  padding-bottom:28rpx;
}
.code image{
  width:240rpx;
  height:240rpx;
  margin:0 auto;
}

